   % \draw [->] (feedback) -- node[name=fb] {$\omega_1(s)$} (motorkonstant);
    \draw [->] (w1) |- node[name=fb2] {} (motorkonstant);
    \draw [->] (motorkonstant) -| node[] {$V_e(s)$} (sum);
    \draw [->] (motorkonstant) -| node[pos=0.95] {$-$} 
        node [near end] {} (sum);
    \draw [] (motorkonstant) -| node[pos=1.25] {$+$} 
        node [near end] {} (sum);
