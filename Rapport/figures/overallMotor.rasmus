\begin{tikzpicture}[auto, node distance=2cm,>=latex']
    % We start by placing the blocks
    \node [input, name=input] {};
    \node [sum, right of=input, node distance=2cm] (sum) {};
    \node [block, right of=sum, align = center] (circuit) {Electrical\\ model};
    \node [block, right of=circuit,
            node distance=2.8cm] (kt) {$K_t$};
    \node [block, right of=kt,  node distance=3cm, align = center] (mech) {Mechanical\\ motor model};
    \node [block, right of=mech,  node distance=3.5cm] (gear) {Gear};
	\node [block, right of=gear,  node distance=2.5cm] (wheel) {Wheel};
