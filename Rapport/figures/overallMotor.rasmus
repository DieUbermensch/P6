    % We draw an edge between the controller and system block to 
    % calculate the coordinate u. We need it to place the measurement block. 
    \node [output, right of=wheel, node distance=2cm] (output) {};

    % Once the nodes are placed, connecting them is easy. 
    \draw [draw,->] (input) -- node {$V_a(s)$} (sum);
    \draw [->] (sum) -- node {} (circuit);
    \draw [->] (circuit) -- node [name=i] {$I_a(s)$}(kt);
    \draw [->] (kt) -- node[name=t] {$\tau_m(s)$} (mech);
    \draw [->] (mech) -- node[name=w1] {$\omega_m(s)$} (gear);
    \draw [->] (gear) -- node[name=t] {$\omega_w(s)$} (wheel);
    \draw [->] (wheel) -- node[name=t] {$F_F(s)$} (output);
    
   % \node [block, below of=i] (feedback) {$\frac{1}{N}$};
    \node [block, below of=i] (motorkonstant) {$K_e$};
